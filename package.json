{
  "name": "desafio-mb-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext \"**/*.{js,jsx,vue}\" --no-error-on-unmatched-pattern ",
    "lint:fix": "eslint src --ext \"**/*.{js,jsx,vue}\" --fix --no-error-on-unmatched-pattern ",
    "format": "npx prettier \"src/**/*.{js,jsx,html,css,scss,vue}\" --write",
    "prepare": "husky install",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pinia/testing": "^0.0.15",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "axios": "^1.3.4",
    "jsdom": "^21.1.1",
    "maska": "^1.5.0",
    "pinia": "^2.0.33",
    "sass": "^1.60.0",
    "v-mask": "^2.3.0",
    "vee-validate": "^4.8.4",
    "vue": "^3.2.47",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "4",
    "vue3-snackbar": "^2.1.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.7",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  }
}
